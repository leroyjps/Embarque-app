{
  "expo": {
    "name": "Embarque-app",
    "slug": "embarque-app",
    "platforms": [
      "android",
      "ios",
      "web"
    ],
    "android": {
      "package": "com.seunome.embarqueapp",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.seunome.embarqueapp"
    },
    "extra": {
      "eas": {
        "projectId": "6b9bb697-ba75-4ee5-b3fc-f222c9225af3"
      }
    }
  }
}
